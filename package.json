{
  "name": "forex-factory",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "turbo run build",
    "check-types": "tsc --noEmit",
    "clean:workspaces": "turbo run clean",
    "dev": "turbo run dev",
    "postinstall": "prisma generate --schema=packages/db/src/schema.prisma",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "studio": "prisma studio --schema=packages/db/src/schema.prisma",
    "db:push": "prisma db push --schema=packages/db/src/schema.prisma",
    "db:generate": "prisma generate --schema=packages/db/src/schema.prisma",
    "bot:start": "pnpm --filter discord-bot start",
    "bot:scheduler": "pnpm --filter discord-bot start:scheduler",
    "bot:all": "pnpm --filter discord-bot start:all",
    "bot:deploy": "pnpm --filter discord-bot discord:deploy",
    "dev:scheduler": "pnpm --filter discord-bot dev:scheduler"
  },
  "devDependencies": {
    "@prisma/client": "^6.4.1",
    "prettier": "^3.0.0",
    "prisma": "^6.4.1",
    "turbo": "^2.5.8",
    "typescript": "^5.1.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.19.0"
  }
}
